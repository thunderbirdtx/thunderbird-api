generator client {
  provider = "prisma-client-js"
   binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Transaction {
  id                String   @id @default(cuid())
  hash              String   @unique
  from              String
  to                String
  value             String
  gasUsed           String
  effectiveGasPrice String
  totalEthUsed      String
  blockNumber       Int
  status            String
  createdAt         DateTime @default(now())
}
